{
  "name": "one-click-hugo-cms",
  "version": "1.0.0",
  "description": "Hugo template with Decap CMS ",
  "repository": "decaporg/one-click-hugo-cms",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src cypress",
    "start": "run-p start:**",
    "start:hugo": "hugo -d dist -w",
    "start:webpack": "webpack-dev-server --config webpack.dev.js --hot",
    "preview": "run-p preview:**",
    "preview:hugo": "yarn run start:hugo -- -D -F",
    "preview:webpack": "yarn run start:webpack",
    "prebuild": "rimraf dist",
    "build": "yarn run build:webpack && hugo -d dist",
    "build:preview": "yarn run build:webpack && hugo -d dist -v -D -F",
    "build:hugo": "hugo -d dist -v",
    "build:hugo:preview": "yarn run build:hugo -- -D -F",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "cypress:open": "cypress open --e2e",
    "cypress:run": "cypress run --e2e",
    "hugo-server": "hugo server",
    "build-preview": "yarn build --mode=production",
    "dev": "NODE_ENV=development npx tailwindcss -i ./assets/css/main.css -o ./assets/css/compiled/main.css --jit -w",
    "build:css": "NODE_ENV=production npx tailwindcss -i ./assets/css/main.css -o ./assets/css/compiled/main.css --jit --minify",
    "postinstall": "vendor-copy",
    "assets": "rimraf assets/vendor && vendor-copy",
    "serve": "run-p serve:**",
    "serve:hugo": "hugo server --disableFastRender",
    "serve:tailwind": "yarn dev"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.16.0",
    "@babel/register": "^7.16.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "assets-webpack-plugin": "^7.1.1",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.2.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.32.2",
    "exports-loader": "^3.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "imports-loader": "^3.1.1",
    "mini-css-extract-plugin": "^2.4.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "postcss-custom-media": "^8.0.2",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "^7.7.2",
    "sass": "^1.53.0",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3",
    "terser-webpack-plugin": "^5.2.5",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "webpack-merge": "^5.8.0",
    "chart.js": "^4.4.9",
    "fuse.js": "^7.1.0",
    "jquery": "^3.7.1",
    "katex": "^0.16.22",
    "lite-youtube-embed": "^0.3.3",
    "medium-zoom": "^1.1.0",
    "mermaid": "^11.6.0",
    "packery": "^3.0.0",
    "tailwind-scrollbar": "^4.0.2",
    "tw-elements": "2.0.0",
    "typeit": "^8.8.7",
    "vendor-copy": "^3.0.1"
  },
  "dependencies": {
    "@babel/preset-react": "^7.17.12",
    "cypress": "^10.5.0",
    "date-fns": "^2.28.0",
    "decap-cms-app": "^3.0.2",
    "immutable": "^4.1.0",
    "netlify-plugin-cypress": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "to-string-loader": "^1.2.0"
  },
  "vendorCopy": [],
  "devVendorCopy": [
